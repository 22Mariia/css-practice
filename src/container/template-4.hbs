      
       <header class="header ">
         <div class="">
            {{> logo}}
            </div>

            <nav class="header_nav ">
                <div class="header_nav_right">
                {{> nav list=header}}
                </div>
            </nav>
       </header>

  {{#with main}}  
<div class="hero">
    <img src="{{image}}" alt="hero image" class="hero_image">
    <div class="hero_content">
        <h1 class="hero_title">{{heading}}</h1>
        <p class="hero_text">{{descriotion}}</p>
        <a class="button card_button" href="{{href}}">{{button}}</a>
    </div>
    
</div>
{{/with}}

<div class="featured">
    {{#each featured}}
{{> cardFeatured data=this}}
    {{/each}}
</div>


<div class="actions">
    {{#each actions}}
   {{> cardActions data=this}}
   {{/each}}
   </div>
   
      


       
       <footer class=" footer">
<div >
{{> logo}}
<span class="footer_copiright">Copyright 2023-2024</span>
</div>
{{#each footer}}
<div >
    <span class="footer_title">{{title}}</span>
      {{> nav list=links isVertical=true}}
</div>
{{/each}}
       </footer>

  
