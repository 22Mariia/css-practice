      
       <header class="header ">
         <a href="#test">
            {{> logo}}
            </a>

            <nav class="header_nav ">
                <div class="header_nav_right">
                {{> nav list=header}}
                </div>
            </nav>
       </header>
    
<main class="main">
  {{#with info}}
    <div class="info">
    <h1 class="info_title">{{title}}</h1>
 
    <p class="info_text">{{description}}</p>
      </div>
  {{/with}}

  {{#with form}} 
<form class="form">
  <div class="form_input_list">
    {{#each inputs}}
    <input type="{{type}}" placeholder="{{placeholder}}" class="form_input" name="{{name}}" id="form_input-{{id}}" ></input>
    {{/each}}
</div>
<div class="form_checkbox">
   
    <input type="checkbox" name="{{checkbox.name}}" id="{{checkbox.name}}">
     <div></div>
    <label for="{{checkbox.name}}">{{checkbox.label}}</label>
</div>

    <button type="submit" class="form_button">{{button}}</button>

    <div class="form_line"></div>
    <p class="form_info">{{info}}</p>
<form>
  {{/with}}
  </main>

<div class="updates">
    {{#with updates}}
     <h2 class="updates_title">{{title}}</h2>
        {{#each list}}
    <div class="updates_list">
        <strong class="updates_name">{{nickname}}</strong>
        <p class="updates_text">{{description}}</p>
    </div>
    {{/each}}
       
         <a href="{{show_all.url}}" class="updates_link">{{show_all.text}}</a>
       
    {{/with}}
</div>

<div class="expirience">
    {{#with expirience}}
    <h3 class="expirience_title">{{title}}</h3>
<div class="expirience_list">
    {{#each list}}
    <div>
        <strong class="expirience_list_name">{{name}}</strong>
        <p class="expirience_list_detailes">{{details}}</p>
    </div>
    {{/each}}
    </div>
    {{/with}}
</div>




 <footer class=" footer" id="test">
<div >
{{> logo}}
<span class="footer_copiright">Copyright 2023-2024</span>
</div>
{{#each footer}}
<div >
    <span class="footer_title">{{title}}</span>
      {{> nav list=links isVertical=true}}
</div>
{{/each}}
       </footer>

  
